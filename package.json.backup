{
  "name": "goodwill-medical-supplies",
  "version": "1.0.0",
  "private": true,
  "description": "Goodwill Medical Supplies - eCommerce Platform",
  "author": "Goodwill Medical",
  "license": "MIT",
  "scripts": {
    "dev": "NEXT_DISABLE_SWC=1 next dev -H 0.0.0.0",
    "build": "NEXT_DISABLE_SWC=1 next build",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "clean": "rm -rf .next node_modules",
    "sitemap": "node scripts/generate-sitemap.js",
    "postbuild": "npm run sitemap"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.7.0",
    "@react-email/components": "^1.0.7",
    "@stripe/react-stripe-js": "2.4.0",
    "@stripe/stripe-js": "2.2.0",
    "bcryptjs": "2.4.3",
    "framer-motion": "10.16.16",
    "micro": "10.0.1",
    "next": "13.5.8",
    "next-auth": "4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-email": "^5.2.8",
    "react-hook-form": "7.48.2",
    "react-hot-toast": "2.4.1",
    "react-query": "3.39.3",
    "resend": "^6.9.2",
    "stripe": "14.10.0",
    "swiper": "11.0.5",
    "zod": "3.22.4",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.8",
    "@types/bcryptjs": "2.4.6",
    "@types/micro": "7.3.7",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.56.0",
    "eslint-config-next": "13.5.8",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.21",
    "prettier": "3.1.1",
    "prisma": "5.7.0",
    "tailwindcss": "3.3.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
